        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:text="@string/textview"
        android:textColor="@color/grey"
        android:textSize="14sp"
        tools:ignore="RtlSymmetry,TextContrastCheck" />

    <ScrollView
